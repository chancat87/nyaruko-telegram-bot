{
  "name": "nyaruko-telegram-bot",
  "version": "0.0.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/niracler/nyaruko-telegram-bot.git"
  },
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit-changes": "git commit -am \"chore: release new version\" || true",
    "tag": "git tag v$(node -p \"require('./package.json').version\")",
    "release": "npm run release:version ${1} && npm run changelog && npm run commit-changes && npm run tag && npm run release:push",
    "release:version": "npm version ${1:-patch} -m \"chore(release): release %s\"",
    "release:push": "git push && git push --tags"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230419.0",
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.10.0",
    "conventional-changelog-cli": "^4.1.0",
    "typescript": "^5.0.4",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "oauth-1.0a": "^2.2.6",
    "openai": "^4.20.1",
    "telegramify-markdown": "git+ssh://git@github.com/niracler/telegramify-markdown.git#master"
  }
}
